<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Response</title>
</head>
<body>
    <h1>These are your emails:</h1>
    <% emails.forEach(function(email){ %>
        <h3><%= email.messageId %></h3>
        <h3><%= email.sender %></h3>
        <h2><%= email.subject %></h2>
        <h3><%= email.summary %></h3>
    <% }) %>
</body>
</html>